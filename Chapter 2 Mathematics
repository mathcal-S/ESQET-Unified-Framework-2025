\chapter{Mathematical Framework}

\section{ESQET Field Equation}
\[
\left(\frac{1}{c^2}\frac{\partial^2}{\partial t^2} - \nabla^2\right) \mathcal{S} = \left( G_0 \cdot \frac{G}{c^2} \right) \cdot \left(\rho_M + \frac{\mathcal{E}_{EM}}{c^2} + \rho_{DM} + \rho_{DE}\right) \cdot \mathcal{F}_{QC}
\]

\section{Quantum Coherence Function}
\[
\mathcal{F}_{QC} = \left(1 + \varphi \cdot \pi \cdot \delta \cdot \frac{\mathcal{D}_{ent} \cdot I_0}{k_B T_{vac}}\right)
\cdot \left(1 + \alpha_{dark} \cdot \frac{\rho_{DM}+\rho_{DE}}{\rho_{total}}\right)
\]
